
public class Runner {
	
	public static void main(String[] args) {
		
		
		int a = 10;
		
		int b = 20;
		
		System.out.println(Calculation(a,b));
		
		
	}
	
	
	static int Calculation(int a, int b) {

		int sum = a + b;
		
		int sub = a - b;
		
		int mult = a * b;
		
		double div = a / b;

		return sub;

	}
	

}
