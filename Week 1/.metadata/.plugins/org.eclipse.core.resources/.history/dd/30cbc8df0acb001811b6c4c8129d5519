public class Runner {

	public static void main(String[] args) {

		String FirstString = "Peter";

		String SecondString = "John";
		
		checkName(FirstString, SecondString);

	}

	static String checkName(String alpha, String beta) {
		if (alpha.equals(beta)) {
			return "Both names Match";
		} else {
			return "They don't Match";
		}

	}

}
