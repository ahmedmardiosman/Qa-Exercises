package com.qa.Garage;

public abstract class Vehicle {

	String brand;
	int numberOfWheels;
	String typeOfFuel;
	String colour;
	int numberOfDoors;
	int numberOfSeats;
	int topSpeed;
	double engineSize;
	
	

	public void sitDown() {
		
		System.out.println("You can sit down in your Vehicle");
	}

	public void playMusic() {
		System.out.println("You can play music in the Vehicle");
	}

}
